{
  "name": "levelgraph",
  "version": "0.8.1",
  "description": "A graph database for Node.js and the browser built on top of LevelUp",
  "main": "lib/levelgraph.js",
  "scripts": {
    "test": "mocha --recursive --bail --reporter spec test",
    "zuul": "zuul -- test",
    "coverage": "rm -rf coverage; istanbul cover _mocha -- --recursive --reporter spec --bail",
    "publish-coverage": "cat coverage/lcov.info | coveralls",
    "ci": "mocha --recursive --bail --watch test",
    "jshint-lib": "jshint lib/*.js",
    "jshint-test": "jshint test/*.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mcollina/levelgraph.git"
  },
  "bugs": {
    "url": "http://github.com/mcollina/levelgraph/issues"
  },
  "pre-commit": [
    "jshint-lib",
    "jshint-test",
    "test"
  ],
  "keywords": [
    "leveldb",
    "graph",
    "level",
    "database",
    "triples",
    "triple"
  ],
  "engines": {
    "node": ">= 0.10.0"
  },
  "author": "Matteo Collina <hello@matteocollina.com>",
  "license": "MIT",
  "devDependencies": {
    "mocha": "~1.16.2",
    "chai": "~1.8.1",
    "sinon": "~1.7.3",
    "sinon-chai": "~2.4.0",
    "jshint": "~2.4.1",
    "level-test": "~1.5.2",
    "uglify-js": "~2.4.8",
    "browserify": "~3.17.0",
    "levelup": "~0.18.0",
    "pre-commit": "0.0.4",
    "zuul": "~1.3.0",
    "coveralls": "~2.6.1",
    "istanbul": "~0.2.3",
    "osenv": "0.0.3",
    "level-sublevel": "~5.2.0"
  },
  "dependencies": {
    "xtend": "~2.1.1",
    "callback-stream": "~1.0.0",
    "async": "~0.2.9",
    "readable-stream": "~1.0.17",
    "level-writestream": "~0.1.1",
    "levelup": ">= 0.10.0 < 0.19.0",
    "leveldown": "> 0.7.0 < 0.11.0",
    "level-js": "~1.0.8",
    "stream-browserify": "~0.1.2"
  },
  "browser": {
    "./lib/getdb.js": "./lib/getdb-browser.js",
    "level-writestream": false
  },
  "testling": {
    "browsers": [
      "ie/latest",
      "firefox/latest",
      "chrome/latest",
      "opera/latest"
    ],
    "harness": "mocha",
    "files": "test/*.js"
  }
}
