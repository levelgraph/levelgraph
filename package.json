{
  "name": "levelgraph",
  "version": "0.8.2",
  "description": "A graph database for Node.js and the browser built on top of LevelUp",
  "main": "lib/levelgraph.js",
  "scripts": {
    "test": "mocha --recursive --bail --reporter spec test",
    "zuul": "zuul -- test",
    "coverage": "rm -rf coverage; istanbul cover _mocha -- --recursive --reporter spec --bail",
    "publish-coverage": "cat coverage/lcov.info | coveralls",
    "ci": "mocha --recursive --bail --watch test",
    "jshint-lib": "jshint lib/*.js",
    "jshint-test": "jshint test/*.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mcollina/levelgraph.git"
  },
  "bugs": {
    "url": "http://github.com/mcollina/levelgraph/issues"
  },
  "pre-commit": [
    "jshint-lib",
    "jshint-test",
    "test"
  ],
  "keywords": [
    "leveldb",
    "graph",
    "level",
    "database",
    "triples",
    "triple"
  ],
  "engines": {
    "node": ">= 0.10.0"
  },
  "author": "Matteo Collina <hello@matteocollina.com>",
  "license": "MIT",
  "devDependencies": {
    "browserify": "~6.1.0",
    "chai": "^1.9.2",
    "coveralls": "^2.11.2",
    "istanbul": "~0.3.2",
    "jshint": "~2.5.6",
    "level-sublevel": "^6.3.15",
    "level-test": "git+https://github.com/mcollina/level-test.git#bump-deps",
    "levelup": "~0.19.0",
    "mocha": "^1.16.2",
    "osenv": "^0.1.0",
    "pre-commit": "0.0.9",
    "sinon": "~1.10.3",
    "sinon-chai": "^2.6.0",
    "uglify-js": "~2.4.8",
    "zuul": "^1.11.2"
  },
  "dependencies": {
    "xtend": "~4.0.0",
    "callback-stream": "~1.0.0",
    "async": "~0.9.0",
    "readable-stream": "~1.0.17",
    "level-write-stream": "~1.0.0",
    "levelup": ">= 0.10.0 < 0.20.0",
    "leveldown": "> 0.7.0 < 0.11.0",
    "level-js": "~2.1.6"
  },
  "browser": {
    "./lib/getdb.js": "./lib/getdb-browser.js"
  },
  "testling": {
    "browsers": [
      "ie/latest",
      "firefox/latest",
      "chrome/latest",
      "opera/latest"
    ],
    "harness": "mocha",
    "files": "test/*.js"
  }
}
